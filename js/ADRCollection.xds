            "userConfig": {
                "title": "Manage"
            },
            "userXType": "manage",
            "cn": [
                {
                    "id": "MyPanel3",
                    "cid": "panel",
                    "jsClass": "MyPanel3",
                    "snapToGrid": 10,
                    "userConfig": {
                        "title": "Collection Properties",
                        "frame": true
                    },
                    "cn": [
                        {
                            "id": "MyToolbar",
                            "cid": "toolbar",
