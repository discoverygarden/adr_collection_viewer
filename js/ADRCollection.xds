                "activeTab": 2,
                "width": 960,
                "height": 900,
                "id": "adr-collection-tabpanel"
            }
        },
        {
            "id": "MyPanel",
            "cid": "panel",
            "jsClass": "Overview",
            "snapToGrid": 10,
            "userConfig": {
                "title": "Overview",
                "autoLoad": "{\nurl: \"/adrcollection/ajax/getdescription\",\nparams: {  \npid: (window.location.pathname.split('/')[3])\n}\n}",
                "id": "adr-collection-overview-tab"
            },
            "userXType": "overview"
        },
        {
            "id": "MyPanel2",
            "cid": "panel",
